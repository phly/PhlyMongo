{
    "name": "phly/phly-mongo",
    "description": "ZF2 Module providing services and paginators surrounding MongoDB",
    "type": "library",
    "keywords": [
        "zf2",
        "zend",
        "module",
        "mongo",
        "mongodb"
    ],
    "homepage": "https://github.com/weierophinney/PhlyMongo",
    "authors": [
        {
            "name": "Matthew Weier O'Phinney",
            "email": "matthew@weierophinney.net",
            "homepage": "http://mwop.net/"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "ext-mongo": "*",
        "zendframework/zend-paginator": "2.*",
        "zendframework/zend-servicemanager": "2.*",
        "zendframework/zend-stdlib": "2.*",
        "zendframework/zend-i18n": "2.*"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "*@dev",
        "phpunit/phpunit": "~3.7",
        "zendframework/zend-form": "2.*",
        "zendframework/zend-log": "2.*",
        "zendframework/zend-modulemanager": "2.*",
        "zendframework/zend-mvc": "2.*",
        "zendframework/zend-serializer": "2.*",
        "zendframework/zend-view": "2.*"
    },
    "autoload": {
        "psr-4": {
            "PhlyMongo\\": "src/",
            "PhlyMongoTest\\": "test/"
        },
        "classmap": [
            "Module.php"
        ]
    }
}
